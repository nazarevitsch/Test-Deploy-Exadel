<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xmlns:pro="http://www.liquibase.org/xml/ns/pro"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/pro http://www.liquibase.org/xml/ns/pro/liquibase-pro-4.1.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.1.xsd"
>
    <changeSet id="create_used_discount_table" author="nazarii">
        <createTable tableName="used_discount">
            <column defaultValueComputed="uuid_generate_v4()" name="ud_id" type="UUID">
                <constraints nullable="false" primaryKey="true" primaryKeyName="pk_used_discount"/>
            </column>
            <column name="ud_discount_id" type="UUID">
                <constraints nullable="false"/>
            </column>
            <column name="ud_user_id" type="UUID">
                <constraints nullable="false" />
            </column>
            <column name="ud_using_date" type="timestamptz"/>
        </createTable>
    </changeSet>
</databaseChangeLog>